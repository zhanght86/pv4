<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
	<class name="com.lcjr.pvxp.orm.Oplog" table="zzt_oplog" dynamic-update="false">
    <composite-id>
				<key-property name="bankid" column="bankid" type="string"/>
				<key-property name="operid" column="operid" type="string"/>
				<key-property name="opdate" column="opdate" type="string"/>
				<key-property name="optime" column="optime" type="string"/>
				<key-property name="type" column="type" type="string"/>
				<key-property name="trcd" column="trcd" type="string"/>
				<key-property name="info" column="info" type="string"/>
		</composite-id>

		<property name="remark1" column="remark1" type="string"/>
		<property name="remark2" column="remark2" type="string"/>
	</class>
</hibernate-mapping>
