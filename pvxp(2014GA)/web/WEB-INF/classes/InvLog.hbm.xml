<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
	<class name="com.lcjr.pvxp.orm.InvLog" table="zzt_invlog" dynamic-update="false">
    <composite-id>
			<key-property name="devno" column="devno" type="string"/>
			<key-property name="pdate" column="pdate" type="string"/>
			<key-property name="ptime" column="ptime" type="string"/>
			<key-property name="invoiceno" column="invoiceno" type="string"/>
		</composite-id>


		<property name="tellerno" column="tellerno" type="string"/>
		<property name="type" column="type" type="string"/>
		<property name="accno1" column="accno1" type="string"/>
		<property name="accno2" column="accno2" type="string"/>
		<property name="money" column="money" type="string"/>
		<property name="printresult" column="printresult" type="string"/>
		<property name="printtoken" column="printtoken" type="string"/>
		<property name="remark1" column="remark1" type="string"/>
		<property name="remark2" column="remark2" type="string"/>
		<property name="remark3" column="remark3" type="string"/>

	</class>
</hibernate-mapping>
