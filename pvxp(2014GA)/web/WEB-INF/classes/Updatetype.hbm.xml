<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="com.lcjr.pvxp.orm.Updatetype" table="zzt_updatetype" dynamic-update="false">
        <id name="updateno" column="updateno" type="string" unsaved-value="any">
            <generator class="assigned"/>
        </id>
	<property name="updatename" column="updatename" type="string"/>
	<property name="info" column="info" type="string"/>
	<property name="remark1" column="remark1" type="string"/>
	<property name="remark2" column="remark2" type="string"/>
	<property name="remark3" column="remark3" type="string"/>

    </class>
</hibernate-mapping>
